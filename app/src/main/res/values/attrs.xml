<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BlueTextView">
        <attr name="maxLines" format="integer" />
        <attr name="emojiconSize" format="dimension" />
        <attr name="rtlSupport" format="boolean" />
        <attr name="emojiconAlignment" format="enum">
            <enum name="bottom" value="0" />
            <enum name="baseline" value="1" />
        </attr>
        <attr name="emojiconTextStart" format="integer" />
        <attr name="emojiconTextLength" format="integer" />
        <attr name="emojiconUseSystemDefault" format="boolean" />
    </declare-styleable>
    <declare-styleable name="DoubleSwitchView">
        <attr name="autochange" format="boolean" />
    </declare-styleable>
    <declare-styleable name="VerificationCodeView">
        <attr name="verify_count" format="integer" />
        <attr name="verify_textsize" format="dimension" />
        <attr name="verify_child_width" format="dimension" />
        <attr name="verify_child_height" format="dimension" />
        <attr name="verify_color_border_focus" format="reference" />
        <attr name="verify_color_border_normal" format="reference" />
        <attr name="verify_inputType" format="string" />
    </declare-styleable>
    <declare-styleable name="PhoneNumberView">
        <attr name="number_textsize_max" format="dimension" />
        <attr name="number_textsize_min" format="dimension" />
        <attr name="color_country_code" format="color" />
        <attr name="color_phone_number" format="color" />
        <attr name="number_bold" format="boolean" />
    </declare-styleable>
    <declare-styleable name="ZTabLayout">
        <attr name="ztab_color" format="color" />
        <attr name="ztab_textcolor" format="color" />
        <attr name="ztab_fulltab" format="boolean" />
    </declare-styleable>
    <declare-styleable name="BlueFaceView">
        <attr name="BlueFaceView_radius" format="dimension" />
        <attr name="BlueFaceView_backgroundColor" format="color" />
        <attr name="BlueFaceView_firstName" format="string" />
        <attr name="BlueFaceView_lastName" format="string" />
        <attr name="BlueFaceView_radiusStatus" format="dimension" />
        <attr name="BlueFaceView_percentText" format="float" />
        <attr name="BlueFaceView_widthOfStatus" format="dimension" />
        <attr name="BlueFaceView_heightOfStatus" format="dimension" />
        <attr name="BlueFaceView_offsetX" format="dimension" />
        <attr name="BlueFaceView_offsetY" format="dimension" />
        <attr name="BlueFaceView_statusDrawable" format="reference" />
        <attr name="BlueFaceView_statusColor" format="color" />
        <attr name="BlueFaceView_supportStatus" format="boolean" />
        <attr name="BlueFaceView_useCircleBackground" format="boolean" />
        <attr name="BlueFaceView_useRoundBreach" format="boolean" />
    </declare-styleable>
    <declare-styleable name="PullLoadMoreView">
        <attr name="top_background_color" format="color" />
    </declare-styleable>
    <declare-styleable name="DiscreteScrollView">
        <attr name="dsv_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="ExpandableLayout">
        <attr name="el_duration" format="integer" />
        <attr name="el_expanded" format="boolean" />
        <attr name="el_parallax" format="float" />
        <attr name="android:orientation" />
    </declare-styleable>
    <declare-styleable name="RoundImageView">
        <attr name="radius" format="dimension" />
    </declare-styleable>
</resources>