<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ChatImageView">
        <attr name="ChatImageView_radius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ChatHeadLayout">
        <attr name="ChatHeadLayout_radius" format="dimension" />
        <attr name="ChatHeadLayout_headWidth" format="dimension" />
        <attr name="ChatHeadLayout_headColor" format="color" />
    </declare-styleable>
    <declare-styleable name="SlideLayoutContent">
        <attr name="SlideLayoutContent_marginSpace" format="dimension" />
    </declare-styleable>
    <declare-styleable name="SlideLayout">
        <attr name="SlideLayout_slideDirection" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

</resources>